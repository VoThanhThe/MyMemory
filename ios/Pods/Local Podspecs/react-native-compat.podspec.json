{
  "name": "react-native-compat",
  "version": "2.12.2",
  "summary": "Shims for WalletConnect Protocol in React Native Projects",
  "homepage": "https://github.com/walletconnect/walletconnect-monorepo/",
  "license": "Apache-2.0",
  "authors": "WalletConnect, Inc. <walletconnect.com>",
  "platforms": {
    "ios": "11.0",
    "visionos": "1.0"
  },
  "source": {
    "git": "https://github.com/walletconnect/walletconnect-monorepo.git",
    "tag": "2.12.2"
  },
  "source_files": "ios/**/*.{h,m,mm}",
  "compiler_flags": "$(inherited) -DFOLLY_NO_CONFIG -DFOLLY_MOBILE=1 -DFOLLY_USE_LIBCPP=1 -DFOLLY_CFG_NO_COROUTINES=1 -Wno-comma -Wno-shorten-64-to-32",
  "dependencies": {
    "React-Core": [

    ],
    "RCT-Folly": [
      "2022.05.16.00"
    ],
    "glog": [

    ]
  },
  "pod_target_xcconfig": {
    "HEADER_SEARCH_PATHS": "\"$(PODS_ROOT)/boost\" \"$(PODS_ROOT)/Headers/Private/Yoga\"",
    "CLANG_CXX_LANGUAGE_STANDARD": "c++20"
  }
}
